name1=input("What is your name?")
name2=input("What is His/Her name?")

combine=name1+name2

lower_case=combine.lower()

t=lower_case.count('t')
r=lower_case.count('r')
u=lower_case.count('u')
e=lower_case.count('e')

true=t+r+u+e

l=lower_case.count('l')
o=lower_case.count('o')
v=lower_case.count('v')
e=lower_case.count('e')

love=l+o+v+e

true_love=int(str(true)+str(love))

if true_love<10 or true_love>90:
    print(f"Your score is {true_love} and you go together like coke and mentos")

elif true_love>=40 and true_love<=50:
    print(f"Your score is {true_love} and you are alright togther")

else:
    print(f"Your love score is {true_love}")

